			file: "../sprites/paths/platform/template8bpp64.png";
			x_offset: -32; y_offset: -33; width: 64; height: 64;
		}
	}

	SUPP {
		tile_width: 64;
		z_height: 16;
		support_type: wood;

		(s_ns, s_ew, _s_ns, _s_ew
		|n#n,  n#e,  n#s,   n#w
		|n#ne, n#es, n#sw,  n#nw
		|n#ns, n#ew, _n#ns, _n#ew
